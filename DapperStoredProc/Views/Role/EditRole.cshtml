@model IEnumerable<DapperStoredProc.Models.RoleEdit>

@{
    Layout = null;

}

<h1>Edit Role</h1>

<form action="@Url.Content("/Role/EditRole")" id="myform" method="post">
    <div class="row">
        <div class="col-md-6">
            <h1>@ViewBag.Email </h1>
        </div>
            <div class="col-md-6">

                @foreach (var item in Model)
                 {
                      Html.RenderPartialAsync("_RoleListPartial", item);
                 }
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Cancel</button>
                <button type="submit" class="btn btn-primary" id="edit" style="background-color:darkgreen;">Submit</button>
            </div>            
    </div>
    
</form>
<script>validateEmpForm()</script>






